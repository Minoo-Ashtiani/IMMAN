string_db_list <- list()
map_list <- list()

for (i in seq_len( list_num)) {
  
  bfc = BiocFileCache(tempdir())
  
  path = bfcnew(bfc, "PPI Downloaded Files")
  
  filenames <- list.files(path, pattern="*.gz", full.names=TRUE)
  
  file<-c()
  for (i in 1:length(filenames)){
    
    s<-stringmerge(Species_IDs[[i]],"__protein_aliases_tf.tsv")
    
    file[[i]] <- filenames[setdiff(names(filenames), s)]
    
  }
  
  string_db_list <- c(string_db_list, list(STRINGdb$new(version = STRINGversion,
                                                        species= Species_IDs[[i]],
                                                        score_threshold = score_threshold)))
  
  
  save(string_db_list, file=path)
}